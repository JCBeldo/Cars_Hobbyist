<%= link_to "Car Index", '/cars' %>
<%= link_to "Hobbyist Index", '/hobbyists' %>
<h1>Cars</h1>
<% @cars.map do |car| %>
  <h2>Make: <%= car.make %></h2>
  <h3>Model: <%= car.model %></h3>
  <p>Drivable: <%= car.drivable %></p>
  <p>Year: <%= car.year %></p>
  <p>Mileage: <%= car.mileage %></p>
<% end %>

