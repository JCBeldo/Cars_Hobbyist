<%= link_to "Car Index", '/cars' %>
<%= link_to "Hobbyist Index", '/hobbyists' %>
<h1>Hobbyists</h1>
<% @hobbyists.map do |hobbyist| %> 
  <h3>Name: <%= hobbyist.name%> <%= hobbyist.created_at %></h3>
  <p>Insurance?: <%= hobbyist.insurance %></p>
  <p>Garage Capacity: <%= hobbyist.garage_capacity %></p>
<% end %> 
